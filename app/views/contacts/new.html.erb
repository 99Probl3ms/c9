<div class="container text-center">
    <h2 class="text-center">Contact Us</h2>
   <%= form_for "/contacts" do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %>
   
   <%= f.label :email %>
   <%= f.text_field :email %>
   
   <%= f.label :comments %>
   <%= f.text_area :comments %>
   <% end %>
</div>